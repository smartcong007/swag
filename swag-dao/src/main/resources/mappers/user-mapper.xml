<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userServiceNameSpace">
  <select id="getUserById" parameterType="java.lang.Integer" resultType="com.cong.swag.common.VO.UserVO">
    SELECT * FROM user
    <where>
      <if test="_parameter != null">
        id=#{id}
      </if>
    </where>
  </select>
</mapper>