<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="auditTypeNameSpace">
  <resultMap id="auditTypeResultMap" type="com.cong.swag.common.VO.AuditTypeVO">
    <result property="id" column="id" />
    <result property="code" column="code" />
    <result property="shopType" column="shop_type" />
  </resultMap>

  <select id="selectList" resultMap="auditTypeResultMap">
    select id,code,shop_type from audit_type_config where delete_at is null
  </select>
</mapper>